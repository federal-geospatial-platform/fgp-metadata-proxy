	  <gmd:transferOptions xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gco="http://www.isotc211.org/2005/gco">
        <gmd:MD_DigitalTransferOptions>
          <gmd:onLine xlink:role="{fme:get-attribute("transfer_option_xlink_role")}">
            <gmd:CI_OnlineResource>
              <gmd:linkage>
                <gmd:URL>{fme:get-attribute("transfer_option_url")}</gmd:URL>
              </gmd:linkage>
              <gmd:protocol>
                <gco:CharacterString>{fme:get-attribute("transfer_option_protocol")}</gco:CharacterString>
              </gmd:protocol>
              <gmd:name xsi:type="gmd:PT_FreeText_PropertyType">
                <gco:CharacterString>{fme:get-attribute("transfer_option_name")}</gco:CharacterString>
                <gmd:PT_FreeText>
                  <gmd:textGroup>
                    <gmd:LocalisedCharacterString locale="fra">{fme:get-attribute("transfer_option_name_other_lang")}</gmd:LocalisedCharacterString>
                  </gmd:textGroup>
                </gmd:PT_FreeText>
              </gmd:name>
              <gmd:description xsi:type="gmd:PT_FreeText_PropertyType">
                <gco:CharacterString>{fme:get-attribute("transfer_option_description_service")};{fme:get-attribute("transfer_option_description_format")};{fme:get-attribute("transfer_option_description_language")}</gco:CharacterString>
                <gmd:PT_FreeText>
                  <gmd:textGroup>
                    <gmd:LocalisedCharacterString locale="fra">{fme:get-attribute("transfer_option_description_service_other_lang")};{fme:get-attribute("transfer_option_description_format")};{fme:get-attribute("transfer_option_description_language_other_lang")}</gmd:LocalisedCharacterString>
                  </gmd:textGroup>
                </gmd:PT_FreeText>
              </gmd:description>
            </gmd:CI_OnlineResource>
          </gmd:onLine>
        </gmd:MD_DigitalTransferOptions>
      </gmd:transferOptions>