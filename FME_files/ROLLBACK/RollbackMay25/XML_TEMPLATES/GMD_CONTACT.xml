  <gmd:contact xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:gco="http://www.isotc211.org/2005/gco">
            <gmd:CI_ResponsibleParty>
              <gmd:individualName xsi:type="gmd:PT_FreeText_PropertyType">
                <gco:CharacterString>{fme:get-attribute("individualName")}</gco:CharacterString>
              </gmd:individualName>
              <gmd:organisationName xsi:type="gmd:PT_FreeText_PropertyType">
                <gco:CharacterString>{fme:get-attribute("p-t_entity")}; {fme:get-attribute("p-t_entity")}; {fme:get-attribute("sector")}</gco:CharacterString>
                <gmd:PT_FreeText>
                  <gmd:textGroup>
                    <gmd:LocalisedCharacterString locale="{fme:get-attribute("p-t_entity_other_lang_locale")}">{fme:get-attribute("p-t_entity_other_lang")}; {fme:get-attribute("p-t_entity_other_lang")}; {fme:get-attribute("sector_other_lang")}</gmd:LocalisedCharacterString>
                  </gmd:textGroup>
                </gmd:PT_FreeText>
              </gmd:organisationName>
              <gmd:positionName xsi:type="gmd:PT_FreeText_PropertyType">
                <gco:CharacterString>{fme:get-attribute("contact_position")}</gco:CharacterString>
                <gmd:PT_FreeText>
                  <gmd:textGroup>
                    <gmd:LocalisedCharacterString locale="{fme:get-attribute("contact_position_other_lang_locale")}">{fme:get-attribute("contact_position_other_lang")}</gmd:LocalisedCharacterString>
                  </gmd:textGroup>
                </gmd:PT_FreeText>
              </gmd:positionName>
              <gmd:contactInfo>
                <gmd:CI_Contact>
					<gmd:phone>
						<gmd:CI_Telephone>
							<gmd:voice xmlns:napm="http://www.geoconnections.org/nap/napMetadataTools/napXsd/napm" xsi:type="gmd:PT_FreeText_PropertyType">
								<gco:CharacterString>{fme:get-attribute("contact_phone")}</gco:CharacterString>
								<gmd:PT_FreeText>
									<gmd:textGroup>
										<gmd:LocalisedCharacterString locale="#fra">{fme:get-attribute("contact_phone")}</gmd:LocalisedCharacterString>
									</gmd:textGroup>
								</gmd:PT_FreeText>
							</gmd:voice>
							<gmd:facsimile>
								<gco:CharacterString>{fme:get-attribute("contact_facsimile")}</gco:CharacterString>
							</gmd:facsimile>
						</gmd:CI_Telephone>
					</gmd:phone>
                  <gmd:address>
                    <gmd:CI_Address>
							<gmd:deliveryPoint xsi:type="gmd:PT_FreeText_PropertyType">
								<gco:CharacterString>{fme:get-attribute("contact_address")}</gco:CharacterString>
								<gmd:PT_FreeText>
									<gmd:textGroup>
										<gmd:LocalisedCharacterString locale="#fra">{fme:get-attribute("contact_address")}</gmd:LocalisedCharacterString>
									</gmd:textGroup>
								</gmd:PT_FreeText>
							</gmd:deliveryPoint>
							<gmd:city>
								<gco:CharacterString>{fme:get-attribute("contact_city")}</gco:CharacterString>
							</gmd:city>
								<gmd:administrativeArea xsi:type="gmd:PT_FreeText_PropertyType">
								<gco:CharacterString>{fme:get-attribute("contact_administrative_area")}</gco:CharacterString>
								<gmd:PT_FreeText>
									<gmd:textGroup>
										<gmd:LocalisedCharacterString locale="#fra">{fme:get-attribute("contact_administrative_area")}</gmd:LocalisedCharacterString>
									</gmd:textGroup>
								</gmd:PT_FreeText>
							</gmd:administrativeArea>
							<gmd:postalCode>
								<gco:CharacterString>{fme:get-attribute("contact_postal_code")}</gco:CharacterString>
							</gmd:postalCode>
							<gmd:country xsi:type="gmd:PT_FreeText_PropertyType">
								<gco:CharacterString>{fme:get-attribute("contact_country")}</gco:CharacterString>
								<gmd:PT_FreeText>
									<gmd:textGroup>
										<gmd:LocalisedCharacterString locale="#fra">{fme:get-attribute("contact_country")}</gmd:LocalisedCharacterString>
									</gmd:textGroup>
								</gmd:PT_FreeText>
							</gmd:country>
                      <gmd:electronicMailAddress xsi:type="gmd:PT_FreeText_PropertyType">
                        <gco:CharacterString>{fme:get-attribute("contact_email")}</gco:CharacterString>
                        <gmd:PT_FreeText>
                          <gmd:textGroup>
                            <gmd:LocalisedCharacterString locale="{fme:get-attribute("contact_email_other_lang_locale")}">{fme:get-attribute("contact_email")}</gmd:LocalisedCharacterString>
                          </gmd:textGroup>
                        </gmd:PT_FreeText>
                      </gmd:electronicMailAddress>
                    </gmd:CI_Address>
                  </gmd:address>
                  <gmd:onlineResource>
					<gmd:CI_OnlineResource>
					  <gmd:linkage>
						<gmd:URL>{fme:get-attribute("online_resource_link")}</gmd:URL>
					  </gmd:linkage>
					  <gmd:protocol>
						<gco:CharacterString>{fme:get-attribute("online_resource_protocol")}</gco:CharacterString>
					  </gmd:protocol>
					  <gmd:description xsi:type="gmd:PT_FreeText_PropertyType">
						<gco:CharacterString>{fme:get-attribute("online_resource_description")}</gco:CharacterString>
						<gmd:PT_FreeText>
						  <gmd:textGroup>
							<gmd:LocalisedCharacterString locale="{fme:get-attribute("online_resource_description_other_lang_locale")}">{fme:get-attribute("online_resource_description_other_lang")}</gmd:LocalisedCharacterString>
						  </gmd:textGroup>
						</gmd:PT_FreeText>
					  </gmd:description>
					</gmd:CI_OnlineResource>
				  </gmd:onlineResource>
                </gmd:CI_Contact>
              </gmd:contactInfo>
              <gmd:role>
                <gmd:CI_RoleCode
                  codeList="http://nap.geogratis.gc.ca/metadata/register/napMetadataRegister.xml#IC_90"
                  codeListValue="{fme:get-attribute("contact_role")}">{fme:get-attribute("contact_role_en")}; {fme:get-attribute("contact_role_fr")}</gmd:CI_RoleCode>
              </gmd:role>
            </gmd:CI_ResponsibleParty>
</gmd:contact>